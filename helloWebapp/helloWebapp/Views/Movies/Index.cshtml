@model IEnumerable<helloWebapp.Models.Movie>
@{
    ViewBag.Title = "Movie";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<table class="table table-bordered table-hover">
    <h1>MOVIE</h1>
    @foreach (var movie in Model)
    {
        <tr>
            <td>@movie.Name</td>


        </tr>
     }
</table>

